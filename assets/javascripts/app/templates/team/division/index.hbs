<div class="row ">
  <div class="col-xs-12 col-md-4 col-lg-3 about">
    <h2 class="text-center">Sekcja {{model.name}}</h2>
    <h3 class="text-center contact"><span class="glyphicon glyphicon-envelop"></span><a {{bindAttr href= 'mailto'}}>Kontakt </a></h3>
  </div>
  <div class="col-xs-12 col-md-8 col-lg-9 members"> 
  <div class="row"> 
    <h3 class="col-xs-12">Członkowie: </h3>
      <div class='col-xs-12 col-md-5 col-md-offset-1 cordinator member'>
        <span class="name" >Imię: {{model.cordinator.name}}<span>
        <p class="aboutCordinator">{{model.cordinator.about}}</p>  
      </div>
        {{#each model.members}}
          <div class='col-xs-12 col-md-5 col-md-offset-1 member'><span class="name" >Imię: {{name}}</span>
          <p class="aboutMember">{{about}}</p>
          </div>
        {{/each}}
  </div>
  </div>
</div>
